body { color: #253340; font: 0.813em/1.462em 'Segoe UI', Tahoma, Arial, Helvetica, sans-serif; }
.cell-highlght { background: #4379bb; }
.action-div { background: none repeat scroll 0 0 #000000; height: 1000px; margin: auto; opacity: 0.8; position: absolute; top: 0; width: 100%; z-index: 1000; }
.info,
.success,
.warning,
.error,
.validation { border: 1px solid; margin: 2px 0px; padding: 5px;/*background-repeat: no-repeat;background-position: 10px center;*/}
.info { color: #00529B; background-color: #DBEEF3; min-height: 100px; width: 133px; float: left; margin: 2px; border-radius: 5px;/*background-image: url('android-icon.jpg');*/}
.success { color: #4F8A10; background-color: #DFF2BF; min-height: 100px; width: 133px; float: left; margin: 2px; border-radius: 5px; /*background-image:url('icon_php.gif');*/}
.warning { color: #9F6000; background-color: #FCD5B4; min-height: 100px; width: 133px; float: left; margin: 2px; border-radius: 5px;/*background-image: url('apple.jpg');*/}
.error { color: #D8000C; background-color: #FFBABA; min-height: 100px; width: 133px; float: left; margin: 2px; border-radius: 5px;/*background-image: url('error.png');*/}
.ui-state-default .ui-icon { background-image: url(images/ui-icons_888888_256x240.png); float: left; }
