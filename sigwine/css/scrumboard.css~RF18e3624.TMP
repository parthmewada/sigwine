.stories-div { width: 12%; float: left; background: #444; position: relative; z-index: 1; }
.todo-div { width: 28%; float: left; min-height: 100px; }
.inprogress-div { width: 28%; float: left; min-height: 100px; }
.testing-div { width: 28%; float: left; min-height: 100px; }
.done-div { width: 16%; float: left; min-height: 100px; }
#sidebarbg { background: #444; border-right: 0px solid #D9D9D9; bottom: 0; left: 0; position: absolute; top: 0; width: 12%; z-index: 0; }
.clearfix { }
.clearfix:before,
.clearfix:after { content: ""; display: table; line-height: 0; }
.clearfix:after { clear: both; }
.content_area { width: 88%; margin-left: 12%; min-height: 100%; }
